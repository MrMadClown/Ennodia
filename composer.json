{
    "name": "mrmadclown/ennodia",
    "description": "Ennodia a PHP Router",
    "keywords": [],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/http-foundation": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.20"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Ennodia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
