{
  "name": "mrmadclown/ennodia",
  "description": "Ennodia a PHP Router",
  "keywords": [],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "symfony/http-foundation": "^5.3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Ennodia\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
